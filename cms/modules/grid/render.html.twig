<div class="grid-module row {{ class|default('') }}" id="{{ id|default('') }}"
     style="{{ bg_color|default(false) ? 'background-color:'~bg_color : '' }}">
    {% for content in contents %}
        <div class="{{ node_row_class|default(node_row_class_custom|default('col')) }}">{{ content|raw }}</div>
    {% endfor %}
</div>
