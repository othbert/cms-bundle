{
    "name": "softspring/cms-bundle",
    "description": "A cms bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "sensio/framework-extra-bundle": "^5.0 || ^6.0",
        "softspring/crudl-controller": "^5.0",
        "softspring/dynamic-form-type": "^5.0",
        "softspring/events": "^5.0",
        "softspring/media-bundle": "^5.0",
        "softspring/polymorphic-form-type": "^5.0",
        "symfony/uid": "^5.0 || ^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.12",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpunit/phpunit": "^9.5",
        "softspring/twig-extra-bundle": "^5.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/maker-bundle": "^1.38",
        "twig/twig": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CmsBundle\\Test\\": "tests"
        }
    },
    "extra": {
    }
}
