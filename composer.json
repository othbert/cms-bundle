{
    "name": "softspring/cms-bundle",
    "description": "A cms bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "softspring/crudl-controller": "^5.0",
        "softspring/polymorphic-form-type": "^5.0",
        "softspring/twig-extra-bundle": "^5.0",
        "symfony/uid": "^5.0 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpunit/phpunit": "^9.5",
        "symfony/config": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CmsBundle\\Test\\": "tests"
        }
    },
    "extra": {
    }
}
