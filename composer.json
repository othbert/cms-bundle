{
    "name": "softspring/cms-bundle",
    "description": "A cms bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.14",
        "sensio/framework-extra-bundle": "^5.4|^6.0",
        "softspring/crudl-controller": "^5.0",
        "softspring/dynamic-form-type": "^5.0",
        "softspring/events": "^5.0",
        "softspring/media-bundle": "^5.0",
        "softspring/polymorphic-form-type": "^5.0",
        "softspring/twig-extra-bundle": "^5.0",
        "symfony/config": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0",
        "symfony/routing": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/uid": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "twig/twig": "^2.10|^3.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "google/cloud-storage": "^1.30",
        "javihgil/doctrine-pagination-bundle": "^2.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5.27",
        "softspring/user-bundle": "^5.0",
        "symfony/maker-bundle": "^1.38",
        "symfony/web-profiler-bundle": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CmsBundle\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
