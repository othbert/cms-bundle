{% trans_default_domain 'sfs_cms' %}

{% extends '@SfsCms/admin_layout.html.twig' %}

{% block breadcrums_content %}
    {{ parent() }}
    <li class="breadcrumb-item"><a href="{{ url('sfs_cms_admin_pages_list') }}">{{ 'admin_pages.list.breadcrumb'|trans }}</a></li>
    <li class="breadcrumb-item active" aria-current="page">{{ 'admin_pages.translations.breadcrumb'|trans }}</li>
{% endblock breadcrums_content %}

{% block content %}
    <h1 class="h3">{{ 'admin_pages.translations.title'|trans }}</h1>

    {% include '@SfsCms/admin/page/_page_tabs.html.twig' with {'current':'translations', 'page':entity} %}

    {% block translations %}
    {% endblock translations %}
{% endblock content %}